{
  "name": "altocumulus",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "functions/**/*",
    "packages/*",
    "packages/libs/*"
  ],
  "scripts": {
    "affected": "npm run build:affected -- --dry",
    "build": "turbo run build",
    "build:affected": "turbo run build --filter=...[main...]",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@altocumulus/eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.11.0"
}