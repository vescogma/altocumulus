{
  "name": "fn-one",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "*": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "emulate": "functions-framework --target=functionOne --signature-type=http",
    "build": "tsup src --format esm,cjs --dts-resolve --no-splitting --clean"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.2"
  },
  "devDependencies": {
    "tsconfig": "*"
  }
}
