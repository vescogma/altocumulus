{
  "name": "@ac/domain-one--fn-two",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "*": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup-node",
    "emulate": "functions-framework --target=domainOneFunctionTwo --signature-type=cloudevent"
  },
  "@ac/deploy": [
    "--gen2",
    "--runtime=nodejs16",
    "--region=us-central1",
    "--source=.",
    "--entry-point=domainOneFunctionTwo",
    "--trigger-topic=test-topic"
  ],
  "dependencies": {
    "@ac/domain-one": "*",
    "@google-cloud/functions-framework": "^3.1.2"
  },
  "devDependencies": {
    "@ac/eslint-config-custom": "*",
    "@ac/tsconfig": "*",
    "tsup": "^6.2.2"
  }
}
