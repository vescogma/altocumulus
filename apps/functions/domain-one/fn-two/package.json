{
  "name": "@ac/domain-one-fn-two",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "*": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src --format esm,cjs --dts-resolve --no-splitting --clean",
    "clean": "rm -rf dist",
    "emulate": "functions-framework --target=domainOneFunctionTwo --signature-type=cloudevent"
  },
  "dependencies": {
    "@ac/domain-one": "*",
    "@ac/eslint-config-custom": "*",
    "@google-cloud/functions-framework": "^3.1.2"
  },
  "devDependencies": {
    "@ac/tsconfig": "*",
    "tsup": "^6.2.2"
  }
}
