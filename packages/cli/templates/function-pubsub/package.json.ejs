{
  "name": "<%- packageName %>",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "*": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup-node",
    "emulate": "functions-framework --target=<%- entryPoint %> --signature-type=cloudevent",
    "test": "jest"
  },
  "@ac/deploy": [
    "--gen2",
    "--runtime=nodejs16",
    "--region=us-central1",
    "--source=.",
    "--entry-point=<%- entryPoint %>",
    "--trigger-topic=<%- pubsubTopic %>"
  ],
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.2"
  },
  "devDependencies": {
    "@ac/eslint-config-custom": "*",
    "@ac/tsconfig": "*",
    "@faker-js/faker": "^7.5.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.1",
    "ts-jest": "^29.0.0-next.0",
    "tsup": "^6.2.2",
    "typescript": "^4.8.2"
  }
}
