{
  "name": "<%- packageName %>",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "*": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup-node",
    "clean": "rm -rf dist",
    "emulate": "functions-framework --target=<%- entryPoint %> --signature-type=http"
  },
  "@ac/deploy": [
    "--gen2",
    "--runtime=nodejs16",
    "--region=us-central1",
    "--source=.",
    "--entry-point=<%- entryPoint %>",
    "--trigger-http",
    "--allow-unauthenticated"
  ],
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.2"
  },
  "devDependencies": {
    "@ac/eslint-config-custom": "*",
    "@ac/tsconfig": "*",
    "tsup": "^6.2.2"
  }
}
